{
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "version": "1.25.3",
    "homepage": "https://golang.org/",
    "license": "BSD",
    "architecture": {
        "64bit": {
            "url": "https://go.dev/dl/go1.25.3.windows-amd64.zip",
            "hash": "SHA256:bc249a599c6fe9d0d4093c363856f6c6320dbbe05e5d5d8818b711fb4a14fc23"
        },
        "arm64": {
            "url": "https://go.dev/dl/go1.25.3.windows-arm64.zip",
            "hash": "SHA256:54aeaffcb70b4269fc97a325319640f44a0dc16b58cce39463014d5be1014233"
        }
    },
    "extension": "zip",
    "force_delete": [
        "${BAULK_VFS}\\go",
        "${BAULK_VFS}\\Temp\\go-build"
    ],
    "venv": {
        "category": "golang",
        "mkdir": [
            "${BAULK_VFS}\\Temp"
        ],
        "path": [
            "${BAULK_PKGROOT}\\bin",
            "${BAULK_VFS}\\go\\bin"
        ],
        "env": [
            "GOPATH=${BAULK_VFS}\\go",
            "GOENV=${BAULK_VFS}\\go\\env",
            "GOCACHE=${BAULK_VFS}\\Temp\\go-build",
            "GOTMPDIR=${BAULK_VFS}\\Temp",
            "GO111MODULE=on",
            "GO_PROXY=https://goproxy.io"
        ]
    }
}
