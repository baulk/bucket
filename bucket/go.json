{
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "version": "1.25.5",
    "homepage": "https://golang.org/",
    "license": "BSD",
    "architecture": {
        "64bit": {
            "url": "https://go.dev/dl/go1.25.5.windows-amd64.zip",
            "hash": "SHA256:ae756cce1cb80c819b4fe01b0353807178f532211b47f72d7fa77949de054ebb"
        },
        "arm64": {
            "url": "https://go.dev/dl/go1.25.5.windows-arm64.zip",
            "hash": "SHA256:55a94a423a6b8f3ac2ac4d05a6e44d7760c6520a2c6dcef7425f6bac79c4eece"
        }
    },
    "extension": "zip",
    "force_delete": [
        "${BAULK_VFS}\\go",
        "${BAULK_VFS}\\Temp\\go-build"
    ],
    "venv": {
        "category": "golang",
        "mkdir": [
            "${BAULK_VFS}\\Temp"
        ],
        "path": [
            "${BAULK_PKGROOT}\\bin",
            "${BAULK_VFS}\\go\\bin"
        ],
        "env": [
            "GOPATH=${BAULK_VFS}\\go",
            "GOENV=${BAULK_VFS}\\go\\env",
            "GOCACHE=${BAULK_VFS}\\Temp\\go-build",
            "GOTMPDIR=${BAULK_VFS}\\Temp",
            "GO111MODULE=on",
            "GO_PROXY=https://goproxy.io"
        ]
    }
}
